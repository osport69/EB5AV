#include <LiquidCrystal.h>

#define RS  6
#define Enable 7
#define D4 2
#define D5 3
#define D6 4
#define D7 5

LiquidCrystal LCD_oscarin(RS, Enable, D4, D5, D6, D7);

byte ninoConBrazoAbajo[] = {
  B01110,
  B01110,
  B01110,
  B00100,
  B11111,
  B00100,
  B01010,
  B10001
};

byte ninoConBrazoArriba[] = {
  B01110,
  B01110,
  B01110,
  B10101,
  B01110,
  B00100,
  B01010,
  B01010
};



void setup()
{
  LCD_oscarin.begin(16,2); 
  LCD_oscarin.createChar(1,ninoConBrazoAbajo);
  LCD_oscarin.createChar(2,ninoConBrazoArriba);
  LCD_oscarin.setCursor(0,0);
  LCD_oscarin.print("OSCAR");
}
void loop()
{
  LCD_oscarin.setCursor(0,02);
  LCD_oscarin.write("2023557");
  
  }
